root = true

[*]
charset = utf-8
insert_final_newline = true
indent_style = tab
trim_trailing_whitespace = true
max_line_length = 120

[*.yml]
indent_style = space

# Reset for log tests
[*.txt]
charset = unset
insert_final_newline = unset
indent_style = unset
trim_trailing_whitespace = false
max_line_length = unset
ij_formatter_enabled = false
